<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>KinesisProducerNet.Linux</PackageId>
    <Authors>Serhii Daletskyi</Authors>
    <TargetFramework>netstandard1.3</TargetFramework>
    <Version>0.12.8-alpha</Version>
    <Title>KinesisProducerNet.Linux</Title>
    <Description>Unofficial Amazon Kinesis Producer Library (KPL) for .NET. This package contains native executable to run on Linux.</Description>
    <PackageProjectUrl>https://github.com/daletskyi/KinesisProducerNet</PackageProjectUrl>
    <PackageTags>amazon kinesis producer</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\amazon-kinesis-producer-native-binaries\linux\kinesis_producer" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Resources\amazon-kinesis-producer-native-binaries\linux\kinesis_producer" />
  </ItemGroup>

</Project>
